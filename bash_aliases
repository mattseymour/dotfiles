alias l="ls -al --color"
alias ll="ls -la --color"
alias openproject="subl --project project.sublime-project"
alias vagrant_rebuild="vagrant destroy -f; vagrant up; vagrant ssh;"
alias c="clear"
alias test_install="python setup.py -p dev test_install"
alias open-project="subl --project"
alias about-pkg="apt-cache show"
alias onescreen="xrandr --output DFP1 --off"
alias twoscreen="xrandr --output DFP1 --mode 1920x1200 --left-of DFP2"
